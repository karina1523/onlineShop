.cookie {
  position: fixed;
  bottom: 20px;
  right: 100px;
  padding: 20px 20px 35px;
  background-color: #d0d0d0;
  width: 350px;
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-align: center;
      -ms-flex-align: center;
          align-items: center;
  font-size: 12px;
  letter-spacing: normal;
  line-height: 16px;
  z-index: 10;
}
@media screen and (max-width: 500px) {
    .cookie {
        width: 80%;
        bottom: 0;
        left: 0;
        -webkit-box-sizing: border-box;
                box-sizing: border-box;
    }
}
@media screen and (max-width: 359px) {
    .cookie {
      padding: 20px 15px 15px;
    }
}
.cookie__text {
    width: 100%;
}
.cookie__link {
    color: #2c3e50;
    text-decoration: underline;
}
.cookie .dots {
    display:none;
}
@media screen and (max-width: 359px) {
    .cookie .dots {
      display: inline-block;
    }
}
.cookie .arrow-down {
    display:none;
}
@media screen and (max-width: 359px) {
    .cookie .arrow-down {
        display: inline-block;
        background-image: url(https://excelsior.fashion/static/img/angle-arrow.png);
        width: 15px;
        height: 15px;
        background-position-x: center;
        background-position-y: 8px;
        background-size: 60%;
        background-repeat: no-repeat;
    }
}
.cookie .material-icons.close {
    position: absolute;
    right: 15px;
    bottom: 5px;
    -ms-flex-negative: 0;
        flex-shrink: 0;
    margin-left: 20px;
    -ms-flex-item-align: start;
        align-self: flex-start;
    border: 1px solid #000;
    padding: 3px 5px;
    cursor: pointer;
}
@media screen and (max-width: 400px) {
    .cookie .material-icons.close {
        /* bottom: 5px;*/
        top: unset;
    }
}
@media screen and (max-width: 359px) {
    .cookie .material-icons.close {
        display: none;
    }
}
.cookie  .material-icons.close.xs {
    display: none;
}
@media screen and (max-width: 359px) {
    .cookie .material-icons.close.xs {
        display: block;
        position: absolute;
        top:5px;
        left: -10px;
        border: none;
        padding: unset;
    }
}
